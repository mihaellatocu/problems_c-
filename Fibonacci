
#include <iostream>
using namespace std;

int main(){
    int n, s0, s1, s;
    s0 = 1;
    s1 =1;
    cin >> n;
    if ((n == 2)||(n == 1)){
        s = 1;
       cout <<s;
        }
    int i = 2;
    if (n > 2){
            while (i < n){
            s = s0 +s1;
            s0 = s1;
            s1 = s;
            ++i;
            }
    cout  << s;
   }
    return 0;
}
